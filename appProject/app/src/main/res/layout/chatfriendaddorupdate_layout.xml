<?xml version="1.0" encoding="utf-8"?>
<com.design.appproject.widget.MyScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/add_box_style"
    android:fillViewport="true">
    <com.design.appproject.widget.MyLinearLayout
        style="@style/add_form_style"
        android:orientation="vertical">
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/uid_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="用户id"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/uid_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="用户id"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/fid_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="好友id"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/fid_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="好友id"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/name_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="名称"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/name_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="名称"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
        <com.design.appproject.widget.MyFlexBoxLayout
            android:id="@+id/picture_ll"
            style="@style/add_item_box_style">
            <com.design.appproject.widget.MyTextView
                android:id="@+id/picture_tv"
                android:text="图片"
                style="@style/add_item_label_style"/>
            <com.design.appproject.widget.MyImageView
                android:id="@+id/picture_ifv"
                style="@style/add_item_img_style"
                android:src="@mipmap/upload"/>
        </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/role_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="角色"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/role_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="角色"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/tablename_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="表名"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/tablename_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="表名"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/alias_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="别名"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/alias_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="别名"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/type_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="类型(0:好友申请，1:好友，2:消息)"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/type_et"
            android:gravity="center_vertical"
            android:inputType="number"
            style="@style/add_item_input_style"
            android:hint="类型(0:好友申请，1:好友，2:消息)"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
        <com.design.appproject.widget.MyFrameLayout
            style="@style/add_btn_box_style">
        <com.design.appproject.widget.MyButton
            android:id="@+id/submit_btn"
            android:text="提交"
            android:gravity="center"
        style="@style/add_btn_submit_style"/>
    </com.design.appproject.widget.MyFrameLayout>
    </com.design.appproject.widget.MyLinearLayout>
</com.design.appproject.widget.MyScrollView>

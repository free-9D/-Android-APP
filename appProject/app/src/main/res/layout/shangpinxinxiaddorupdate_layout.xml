<?xml version="1.0" encoding="utf-8"?>
<com.design.appproject.widget.MyScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/add_box_style"
    android:fillViewport="true">
    <com.design.appproject.widget.MyLinearLayout
        style="@style/add_form_style"
        android:orientation="vertical">
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/shangpinmingcheng_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="商品名称"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/shangpinmingcheng_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="商品名称"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
        <com.design.appproject.widget.MyFlexBoxLayout
            style="@style/add_item_box_style">
            <com.design.appproject.widget.MyTextView
                android:text="商品分类"
                style="@style/add_item_label_style"/>
            <com.design.appproject.widget.BottomSpinner
                android:id="@+id/shangpinfenlei_bs"
                style="@style/add_item_picker_box_style"
                android:text="请选择商品分类"/>
        </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/shangpinpinpai_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="商品品牌"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/shangpinpinpai_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="商品品牌"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/shangpinguige_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="商品规格"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/shangpinguige_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="商品规格"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
        <com.design.appproject.widget.MyFlexBoxLayout
            android:id="@+id/shangpintupian_ll"
            style="@style/add_item_box_style">
            <com.design.appproject.widget.MyTextView
                android:id="@+id/shangpintupian_tv"
                android:text="商品图片"
                style="@style/add_item_label_style"/>
            <com.design.appproject.widget.MyImageView
                android:id="@+id/shangpintupian_ifv"
                style="@style/add_item_img_style"
                android:src="@mipmap/upload"/>
        </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/shangpinjianjie_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="商品简介"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/shangpinjianjie_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="商品简介"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
        <com.design.appproject.widget.MyFlexBoxLayout
            style="@style/add_item_box_style">
            <com.design.appproject.widget.MyTextView
                android:text="商品详情"
                style="@style/add_item_label_style"/>
            <com.design.appproject.widget.MyEditTextView
                android:id="@+id/shangpinxiangqing_et"
                android:hint="商品详情"
                android:gravity="center_vertical"
                style="@style/add_item_textarea_style"/>
        </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/price_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="价格"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/price_et"
            android:gravity="center_vertical"
            android:inputType="number"
            style="@style/add_item_input_style"
            android:hint="价格"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/shangjiazhanghao_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="商家账号"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/shangjiazhanghao_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="商家账号"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/shangjiaxingming_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="商家姓名"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/shangjiaxingming_et"
            android:gravity="center_vertical"
            style="@style/add_item_input_style"
            android:hint="商家姓名"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
        <com.design.appproject.widget.MyFrameLayout
            style="@style/add_btn_box_style">
        <com.design.appproject.widget.MyButton
            android:id="@+id/submit_btn"
            android:text="提交"
            android:gravity="center"
        style="@style/add_btn_submit_style"/>
    </com.design.appproject.widget.MyFrameLayout>
    </com.design.appproject.widget.MyLinearLayout>
</com.design.appproject.widget.MyScrollView>
